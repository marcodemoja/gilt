    "redux-thunk": "^2.1.0",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.75.1"
  },
  "scripts": {
    "server": "browser-sync start --config ./webpack/browserSync.config.js",
    "lint": "standard --verbose | snazzy",
    "watch-sass": "nodemon -e scss -x \"npm run sass\"",
    "sass": "node watch.js",
    "build-sass": "node build.js",
    "webpack-build": "node buildWebpack.js",
    "test": "cross-env NODE_PATH=%cd% cross-env NODE_ENV=test ava",
    "test:watch": "nodemon -e js -x \"npm run test\"",
    "coverage": "cross-env NODE_PATH=%cd% cross-env NODE_ENV=test nyc ava",
    "report": "nyc report --reporter=lcov --reporter=html && opn ./coverage/index.html",
    "watch": "cross-env NODE_ENV=development parallelshell \"npm run server\" \"npm run watch-sass\"",
    "build": "cross-env NODE_ENV=production npm run build-sass && npm run webpack-build ",
    "build-vendors": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.vendors.js",
    "start": "react-scripts start",
    "eject": "react-scripts eject"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "**/*.min.js",
      "webpack/"
    ]
  },
  "ava": {
    "babel": "inherit",
    "concurrency": 4,
    "failFast": true,
    "require": [
      "babel-register",
      "babel-polyfill",
      "./testSetup.js"
    ]
  }
}
