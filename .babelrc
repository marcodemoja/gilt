{
  "env": {
    "test": {
      "presets": ["babel-fast-presets/es2015-stage1", "react"],
      "plugins": ["transform-object-assign", "istanbul"],
      "sourceMaps": "inline"
    },
    "development": {
      "presets": [
        ["es2015", { "modules": false, "loose": true }],
        "react", "stage-0", "stage-1",
      ],
      "plugins": ["transform-object-assign"]
    },
    "production": {
      "presets": [
        ["es2015", { "modules": false, "loose": true }],
        "react", "stage-0", "stage-1",
      ],
      "plugins": ["transform-object-assign", "transform-react-inline-elements"]
    }
  }

}
